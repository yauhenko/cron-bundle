{
	"name": "yauhenko/cron-bundle",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "Cron for Symfony console commands",
	"license": "proprietary",
	"require": {
		"php": "^8.2"
	},
	"require-dev": {
		"symfony/http-kernel": "^6.4|^7",
		"symfony/console": "^6.4|^7",
		"symfony/config": "^6.4|^7",
		"symfony/dependency-injection": "^6.4|^7"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yauhenko\\CronBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "6.4.*|7"
		}
	}
}
